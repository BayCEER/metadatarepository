<?xml version="1.0"?>
<info>
    <id>metadatarepo</id>
    <name>Metadata Repository</name>
    <description>A Metadata Repository for OwnCloud based on lucene backend</description>
    <licence>AGPL</licence>
    <author>Stefan Holzheu, Oliver Archner</author>
    <version>0.1.6</version>
	<types>
		<filesystem/>
	</types>
    <namespace>MDRepo</namespace>
    <category>tool</category>
    <dependencies>
        <owncloud min-version="9" max-version="11" />
    </dependencies>
	<settings>
		<admin>OCA\MDRepo\Settings\Admin</admin>
	</settings>
	<settings-sections>
		<admin>OCA\MDRepo\Settings\AdminSectionOC</admin>
	</settings-sections>
	<background-jobs>
        <job>OCA\MDRepo\BackgroundJob\RemoveDeleted</job>
	</background-jobs>
    
</info>
